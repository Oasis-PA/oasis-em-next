{
  "name": "oasis",
  "author": {
    "name": "stefano"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.cjs",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:db:reset": "dotenv -e .env.test -- npx prisma migrate reset --force",
    "test:db:push": "dotenv -e .env.test -- npx prisma db push",
    "test:db": "dotenv -e .env.test -- node --loader ts-node/esm prisma/test-connection.ts",
    "test:api": "jest tests/api",
    "test:integration": "jest tests/integration",
    "test:specific": "jest --testNamePattern",
    "test:setup": "npm run test:db:reset && npm run test:db:push",
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@react-oauth/google": "^0.12.2",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.0",
    "next-auth": "^4.24.7",
    "oasis": "file:",
    "prisma": "6.15",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.1.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "jest": "^30.1.3",
    "node-mocks-http": "^1.17.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "type": "module"
}
